{
  "name": "appcelerator-titanium",
  "main": "./lib/index",
  "version": "2.2.0",
  "description": "Appcelerator Titanium build tools and UI for Atom",
  "repository": "https://github.com/appcelerator/atom-appcelerator-titanium",
  "license": "Apache-2.0",
  "engines": {
    "atom": ">1.21.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "keywords": [
    "axway",
    "appcelerator",
    "titanium",
    "alloy",
    "mobile",
    "javascript",
    "ios",
    "android"
  ],
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "4.0.0": "provideAutoComplete"
      }
    },
    "hyperclick": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "npm run lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "atomTestRunner": "./spec/test-runner.js",
  "dependencies": {
    "@primer/octicons": "^14.2.2",
    "etch": "^0.12.6",
    "find": "^0.3.0",
    "fs-extra": "^10.0.0",
    "mkdirp": "^1.0.4",
    "semver": "^7.3.5",
    "sudo-prompt": "^9.2.1",
    "titanium-editor-commons": "^1.2.5",
    "underscore": "^1.13.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@atom/mocha-test-runner": "^1.6.1",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@seadub/danger-plugin-dependencies": "^1.0.0",
    "@seadub/danger-plugin-eslint": "^2.0.0",
    "@seadub/danger-plugin-junit": "^0.2.0",
    "@semantic-release/apm-config": "^8.0.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "danger": "^10.6.6",
    "eslint": "^7.32.0",
    "eslint-config-axway": "^6.0.2",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "mocha-jenkins-reporter": "^0.4.6",
    "pinst": "^2.1.6",
    "semantic-release": "^17.4.5",
    "sinon": "^11.1.1"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint --ext .js,.jsx .",
    "release": "cross-env HUSKY=0 semantic-release",
    "test": "apm --version && apm test",
    "postinstall": "husky install || exit 0",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "release": {
    "extends": "@semantic-release/apm-config",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/apm",
      "@semantic-release/git"
    ]
  }
}
